{
  "name": "New Tab Todo",
  "description": "Creates a quick todo list on your new tab",
  "version": "0.1.1",
  "author": "Shane Jones",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "icons": { "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
